{
    "name": "heptacom/heptaconnect-storage-shopware-dal",
    "type": "library",
    "description": "HEPTAconnect package to use shopware DAL as underlying storage system",
    "keywords": [
        "heptaconnect-storage",
        "shopware",
        "shopware-platform"
    ],
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "require": {
        "php": ">=7.4",
        "heptacom/heptaconnect-dataset-base": "@dev",
        "heptacom/heptaconnect-portal-base": "@dev",
        "heptacom/heptaconnect-storage-base": "@dev",
        "shopware/core": ">=6.2.3"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "require-dev": {
        "ext-json": "*",
        "ergebnis/composer-normalize": ">=2.7",
        "friendsofphp/php-cs-fixer": ">=2",
        "icanhazstring/composer-unused": ">=0.7",
        "infection/infection": ">=0.15",
        "maglnet/composer-require-checker": ">=2",
        "phpstan/extension-installer": ">=1",
        "phpstan/phpstan": ">=0.12",
        "phpstan/phpstan-deprecation-rules": ">=0.12",
        "phpstan/phpstan-phpunit": ">=0.12",
        "phpstan/phpstan-strict-rules": ">=0.12",
        "phpunit/phpunit": ">=9.3 <10",
        "psalm/plugin-phpunit": ">=0.10",
        "vimeo/psalm": ">=3"
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Storage\\ShopwareDal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Storage\\ShopwareDal\\Test\\": "test/"
        }
    },
    "minimum-stability": "stable"
}
